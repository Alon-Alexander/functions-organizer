{
  "name": "functions-organizer",
  "displayName": "Functions Organizer",
  "description": "An extension to move and organize functions in a file using shortcuts instead of cut&paste.",
  "version": "1.0.0",
  "publisher": "AlonAlexander",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alon-Alexander/functions-organizer.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:java"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.moveFunctionUp",
        "category": "FO",
        "title": "Move Function Up"
      },
      {
        "command": "extension.moveFunctionDown",
        "category": "FO",
        "title": "Move Function Down"
      },
      {
        "command": "extension.sortFunctions",
        "category": "FO",
        "title": "Sort Functions"
      }
    ],
    "keybindings": [
      {
        "command": "extension.moveFunctionUp",
        "key": "ctrl+shift+up",
        "mac": "ctrl+shift+up",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.moveFunctionDown",
        "key": "ctrl+shift+down",
        "mac": "ctrl+shift+down",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "chai": "^4.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  }
}