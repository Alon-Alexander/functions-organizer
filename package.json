{
  "name": "functions-organizer",
  "displayName": "Functions Organizer",
  "description": "An extension to move and organize functions in a file using shortcuts instead cut-paste.",
  "version": "0.0.1",
  "publisher": "alon008",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.moveFunctionUp",
        "title": "FO: Move Function Up"
      },
      {
        "command": "extension.moveFunctionDown",
        "title": "FO: Move Function Down"
      }
    ],
    "keybindings": [
      {
        "command": "extension.moveFunctionUp",
        "key": "ctrl+shift+up",
        "mac": "ctrl+shift+up",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.moveFunctionDown",
        "key": "ctrl+shift+down",
        "mac": "ctrl+shift+down",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
